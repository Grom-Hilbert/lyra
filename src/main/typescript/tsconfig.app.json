{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "internal/vite-env.d.ts",
    "internal/**/*",
    "internal/**/*.vue"
  ],
  "exclude": ["internal/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./internal/*"]
    },
    "types": ["vite/client", "element-plus/global"]
  }
}
